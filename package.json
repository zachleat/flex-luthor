{
  "name": "@zachleat/flex-luthor",
  "version": "4.0.2",
  "description": "A small wrapper library to help with responsive intrinsic-sized Flexbox layouts that wrap based on content and container width and avoiding viewport-based media queries.",
  "main": "flex-luthor.css",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npx @11ty/eleventy --config=demo-src/eleventy.config.js --input=demo-src",
    "start": "npm run build -- --serve"
  },
  "keywords": [
    "responsive",
    "flexbox",
    "intrinsic",
    "css"
  ],
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0"
  }
}
